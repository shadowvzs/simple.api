<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    Options -Indexes
    #RewriteRule !\.(?:jpe?g|gif|mp3|bmp|png|tiff|css|js)(.*)$  index.php?p=$1 [L,NC,QSA]
    RewriteRule ^(.*)$ index.php?query=$1 [NC,L,QSA]
</IfModule>